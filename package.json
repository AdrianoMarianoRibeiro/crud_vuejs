{
  "name": "crud_vuejs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:https": "node dev-server.js",
    "build": "NODE_OPTIONS=--max_old_space_size=6144 vue-cli-service build",
    "lint": "vue-cli-service lint",
    "cypress": "cypress open",
    "prepare": "husky install",
    "e2e:run": "cypress run -- --spec 'cypress/e2e/**/*'",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "vue-cli-service test:unit",
    "test:clearCache": "npm test -- --clearCache",
    "test:watch": "npm test -- --watchAll",
    "test:coverage": "npm test -- --runInBand --collectCoverage",
    "test:staged": "npm test -- --findRelatedTests",
    "release": "npx standard-version --tag-prefix='' --releaseCommitMessageFormat=\"ci(release): {{currentTag}} [skip ci]\"",
    "changelog": "npx auto-changelog",
    "changelog:json": "npx auto-changelog --template json --output changelog-data.json"
  },
